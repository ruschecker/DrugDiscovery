define = -DPOSRES
integrator = steep 
table-extension = 100
nsteps = 100
constraints = all-bonds
emtol = 10000.0
nstcgsteep = 100 ; do a steep every 100 steps of cg
emstep = 0.01 ; used with steep
nstcomm = 1
coulombtype = PME
ns_type = grid
rlist = 1.0
rcoulomb = 1.0
rvdw = 1.4
Tcoupl = no
Pcoupl = no
gen_vel = no
nstxout = 0 ; write coords every # step
optimize_fft = yes
